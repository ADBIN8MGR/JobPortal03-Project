<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Admin Dashboard</title>
</head>
<body>
  <h1>Welcome to Admin Dashboard, <%= user.username %></h1>
  <button onclick="location.href='/auth/logout'">Logout</button>

  <h2>Job Seekers</h2>
  <ul>
    <% jobSeekers.forEach(seeker => { %>
      <li><strong><%= seeker.username %></strong>: <%= seeker.email %></li>
    <% }); %>
  </ul>

  <h2>Companies</h2>
  <ul>
    <% companies.forEach(company => { %>
      <li><strong><%= company.username %></strong>: <%= company.email %></li>
    <% }); %>
  </ul>
</body>
</html>
